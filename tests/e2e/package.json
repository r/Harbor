{
  "name": "harbor-e2e-tests",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "build:extensions": "cd ../../extension && npm run build && cd ../web-agents-api && npm run build",
    "test:browser": "node run-browser-tests.mjs",
    "test:browser:build": "npm run build:extensions && node run-browser-tests.mjs",
    "test:browser:keep-open": "node run-browser-tests.mjs --keep-open",
    "test:all": "npm run test && npm run test:browser"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.11.0",
    "get-port": "^7.1.0",
    "playwright": "^1.42.0",
    "serve-handler": "^6.1.5",
    "typescript": "^5.3.0",
    "web-ext": "^8.0.0"
  }
}
